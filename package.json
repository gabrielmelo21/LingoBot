{
  "name": "slag-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:android": "npx rimraf slag-app && ng build --output-path=slag-app && npx cap copy android && npx cap sync android && \"C:\\Program Files\\Android\\Android Studio\\bin\\studio64.exe\" android",
